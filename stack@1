2) #include<iostream>
    #include<stack>
    using namespace std;
    int main(){
        stack<int>st1;
        stack<int>st2;
        st1.push(1);
        st1.push(2);
        st1.push(3);
        st1.push(4);
        st1.push(5);
        int k;
        cout<<"enter k : ";
        cin>>k;
        for(int i=1;i<k;i++){
            int x = st1.top();
            st1.pop();
            st2.push(x);
        }
        cout<<st1.top();
        
    }
3) option B.
